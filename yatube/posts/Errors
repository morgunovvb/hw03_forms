    @login_required
    def post_edit(request, post_id):
        is_edit = True
        post = get_object_or_404(Post, pk=post_id)
        if request.method == 'POST':
            form = PostForm(request.POST, instance=post)
            if form.is_valid():
                post = form.save(commit=False)
